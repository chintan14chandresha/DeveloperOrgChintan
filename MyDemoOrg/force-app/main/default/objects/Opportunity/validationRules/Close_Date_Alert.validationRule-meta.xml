<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Close_Date_Alert</fullName>
    <active>true</active>
    <errorConditionFormula>IF(  NOT(IsClosed ) ,  CloseDate  &lt;  TODAY() , false)</errorConditionFormula>
    <errorMessage>Hey McFly, unless you are planning to go back in time, please update your close date&apos;</errorMessage>
</ValidationRule>
