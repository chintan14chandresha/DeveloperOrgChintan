<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Dialpad__ArchiveRule</fullName>
    <active>true</active>
    <errorConditionFormula>AND(NOT(ISNEW()), AND(ISCHANGED(Dialpad__Status__c ) , 
 (ISPICKVAL(PRIORVALUE( Dialpad__Status__c ) , &quot;Archive&quot;))))</errorConditionFormula>
    <errorMessage>Status of Archived list cannot be changed</errorMessage>
</ValidationRule>
